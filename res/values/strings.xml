    <string name="contact_police_hotline">Police Information Hotline</string>
    <string name="contact_police_desc1">Respond to police appeals for information on criminal cases and missing persons.</string>
    <string name="contact_police_desc2">For general inquiries on Police services, you may contact a Neighbourhood Police Centre near you. Please use the NPC/NPP locator module in this App.</string>
    <string name="contact_police_hotline_no">1800-2550000</string>
