    <string name="contact_police_traffic_desc">Respond to police appeals for information on traffic accidents</string>
    <string name="contact_police_traffic_no">6547-0000</string>
    <string name="contact_police_emergency">Police Emergency Hotline</string>
    <string name="contact_police_emergency_desc">Reporting of a crime in progress or a potential crime that might be committed.</string>
    <string name="contact_police_emergency_no">999</string>
    <string name="contact_police_civil">Singapore Civil Defence Force (SCDF)</string>
    <string name="contact_police_civil_desc1">For medical emergencies in life-threatening situations, such as when someone is seriously injured in an accident or suddenly collapses.</string>
    <string name="contact_police_civil_desc2">To report a situation in which fire-fighting and rescue services are required.</string>
    <string name="contact_police_civil_no">995</string>
    <string name="contact_police_non_emergency">Non-Emergency Ambulance Services</string>
    <string name="contact_police_non_emergency_desc">For medical non-emergency cases, which do NOT end in death or serious complication if not treated immediately.</string>
    <string name="contact_police_non_emergency_no">1777</string>
